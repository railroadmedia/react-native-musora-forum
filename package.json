{
  "name": "react-native-musora-forum",
  "version": "0.5.34",
  "description": "Forum component for musora apps.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/javascript/index.d.js",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-musora-forum.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/railroadmedia/react-native-musora-forum",
  "author": "BogdanV, CsillaJ, KentonP <bogdan.r.volintiru@gmail.com> (https://github.com/bogdan-vol)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/railroadmedia/react-native-musora-forum/issues"
  },
  "homepage": "https://github.com/railroadmedia/react-native-musora-forum#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.71.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "axios": "^1.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "pod-install": "^0.1.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-builder-bob": "^0.18.0",
    "react-native-device-info": "^8.4.9",
    "release-it": "^14.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-pell-rich-editor": "^1.9.0",
    "@reduxjs/toolkit": "^1.9.5",
    "react-native-gesture-handler": "^2.9.0",
    "@native-html/iframe-plugin": "^2.6.1",
    "react-native-render-html": "^6.3.4",
    "react-native-webview": "git+https://github.com/railroadmedia/react-native-webview.git",
    "react-native-svg": "^13.13.0",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-image-crop-picker": "https://github.com/railroadmedia/react-native-image-crop-picker.git"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  }
}
