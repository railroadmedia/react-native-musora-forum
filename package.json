{
  "name": "react-native-musora-forum",
  "version": "0.5.27",
  "description": "Forum component for musora apps.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/javascript/index.d.js",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-musora-forum.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/railroadmedia/react-native-musora-forum",
  "author": "BogdanV, CsillaJ, KentonP <bogdan.r.volintiru@gmail.com> (https://github.com/bogdan-vol)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/railroadmedia/react-native-musora-forum/issues"
  },
  "homepage": "https://github.com/railroadmedia/react-native-musora-forum#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@react-native-async-storage/async-storage": "^1.15.5",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-pell-rich-editor": "^1.7.0",
    "react-native-gesture-handler": "^1.10.3",
    "@native-html/iframe-plugin": "^1.1.2",
    "react-native-render-html": "^5.1.0",
    "react-native-webview": "git+https://github.com/railroadmedia/react-native-webview.git",
    "react-native-svg": "^12.1.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "none",
    "semi": true,
    "arrowParens": "avoid",
    "jsxSingleQuote": true
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  }
}
